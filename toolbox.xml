<category name="HaloHD" colour="#02AD4D" iconclass="blocklyTreeIconCustom robot">
<label text="HaloHD" web-class="myLabelStyle"></label>
 <block type="import_halo_hd"></block>
 <block type="halo_hd_init"></block>
 <block type="halo_hd_set_led">
    <value name="led_number">
        <shadow type="textinline">
            <field name="text">0</field>
        </shadow>
    </value>
    <value name="led_red">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
    <value name="led_green">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
    <value name="led_blue">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
 </block>
 <block type="halo_hd_set_leds">
    <value name="led_red">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
    <value name="led_green">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
    <value name="led_blue">
        <shadow type="textinline">
            <field name="text">64</field>
        </shadow>
    </value>
 </block>  
 <block type="halo_hd_show_leds"></block>
 <block type="halo_hd_clear_leds"></block>
 <block type="halo_hd_read_time"></block>
 <block type="halo_hd_set_time">
    <value name="time_hours">
        <shadow type="textinline">
            <field name="text">12</field>
        </shadow>
    </value>
    <value name="time_minutes">
        <shadow type="textinline">
            <field name="text">15</field>
        </shadow>
    </value>
    <value name="time_seconds">
        <shadow type="textinline">
            <field name="text">30</field>
        </shadow>
    </value>
 </block>
 <block type="halo_hd_read_hours"></block>
 <block type="halo_hd_read_minutes"></block>
 <block type="halo_hd_read_seconds"></block>
</category>
